package com.itheima.redbaby.bean;

import java.io.Serializable;
import java.util.Arrays;
import java.util.List;
import java.util.Map;

import com.itheima.redbaby.bean.goodsentry.GoodsGuoLvBean;

/**
 * 产品的bean
 * 
 * @author zhangyun
 * 
 */
public class Product implements Serializable {
	private String id; // 商品ID
	private String name;
	private String pic; // 商品的图片
	private float marketprice;// 市场价格
	private float price;// 现在价格
	private int commentCount;
	private double score;// 商品评分
	private float limitprice;// 抢购价格
	private int surplus;// 剩余件数
	private long lefttime;// 开始时间
	private String smallpic;// 小图集合
	private String bigPic;// bigpic[url2]大图集合
	private int buyCount;// 已被购买的数量

	private String available;
	private int buyLimit;
	private String inventoryArea;
	private String productProm;
	private String productdesc;
	private int number;
	private int uplimit;
	private boolean isgigt;
	private String[] checkout_prom;
	private Map<String,String> product_propertys;//"颜色":"红色" "大小":"MLK"
	private List<GoodsGuoLvBean> product_property;
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getPic() {
		return pic;
	}
	public void setPic(String pic) {
		this.pic = pic;
	}
	public float getMarketprice() {
		return marketprice;
	}
	public void setMarketprice(float marketprice) {
		this.marketprice = marketprice;
	}
	public float getPrice() {
		return price;
	}
	public void setPrice(float price) {
		this.price = price;
	}
	public int getCommentCount() {
		return commentCount;
	}
	public void setCommentCount(int commentCount) {
		this.commentCount = commentCount;
	}
	public double getScore() {
		return score;
	}
	public void setScore(double score) {
		this.score = score;
	}
	public float getLimitprice() {
		return limitprice;
	}
	public void setLimitprice(float limitprice) {
		this.limitprice = limitprice;
	}
	public int getSurplus() {
		return surplus;
	}
	public void setSurplus(int surplus) {
		this.surplus = surplus;
	}
	public long getLefttime() {
		return lefttime;
	}
	public void setLefttime(long lefttime) {
		this.lefttime = lefttime;
	}
	public String getSmallpic() {
		return smallpic;
	}
	public void setSmallpic(String smallpic) {
		this.smallpic = smallpic;
	}
	public String getBigPic() {
		return bigPic;
	}
	public void setBigPic(String bigPic) {
		this.bigPic = bigPic;
	}
	public int getBuyCount() {
		return buyCount;
	}
	public void setBuyCount(int buyCount) {
		this.buyCount = buyCount;
	}
	public String getAvailable() {
		return available;
	}
	public void setAvailable(String available) {
		this.available = available;
	}
	public int getBuyLimit() {
		return buyLimit;
	}
	public void setBuyLimit(int buyLimit) {
		this.buyLimit = buyLimit;
	}
	public String getInventoryArea() {
		return inventoryArea;
	}
	public void setInventoryArea(String inventoryArea) {
		this.inventoryArea = inventoryArea;
	}
	public String getProductProm() {
		return productProm;
	}
	public void setProductProm(String productProm) {
		this.productProm = productProm;
	}
	public String getProductdesc() {
		return productdesc;
	}
	public void setProductdesc(String productdesc) {
		this.productdesc = productdesc;
	}
	public int getNumber() {
		return number;
	}
	public void setNumber(int number) {
		this.number = number;
	}
	public int getUplimit() {
		return uplimit;
	}
	public void setUplimit(int uplimit) {
		this.uplimit = uplimit;
	}
	public boolean isIsgigt() {
		return isgigt;
	}
	public void setIsgigt(boolean isgigt) {
		this.isgigt = isgigt;
	}
	public String[] getCheckout_prom() {
		return checkout_prom;
	}
	public void setCheckout_prom(String[] checkout_prom) {
		this.checkout_prom = checkout_prom;
	}
	public Map<String, String> getProduct_propertys() {
		return product_propertys;
	}
	public void setProduct_propertys(Map<String, String> product_propertys) {
		this.product_propertys = product_propertys;
	}
	public List<GoodsGuoLvBean> getProduct_property() {
		return product_property;
	}
	public void setProduct_property(List<GoodsGuoLvBean> product_property) {
		this.product_property = product_property;
	}
	public Product() {
		super();
	}
	@Override
	public String toString() {
		return "Product [id=" + id + ", name=" + name + ", pic=" + pic + ", marketprice=" + marketprice + ", price=" + price + ", commentCount=" + commentCount + ", score=" + score + ", limitprice=" + limitprice + ", surplus=" + surplus + ", lefttime="
				+ lefttime + ", smallpic=" + smallpic + ", bigPic=" + bigPic + ", buyCount=" + buyCount + ", available=" + available + ", buyLimit=" + buyLimit + ", inventoryArea=" + inventoryArea + ", productProm=" + productProm + ", productdesc="
				+ productdesc + ", number=" + number + ", uplimit=" + uplimit + ", isgigt=" + isgigt + ", checkout_prom=" + Arrays.toString(checkout_prom) + ", product_propertys=" + product_propertys + ", product_property=" + product_property + "]";
	}
	

	
	
	
}
