<?xml version="1.0" encoding="utf-8"?><RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:background="@color/backgroundcolor"    android:orientation="vertical" >        <include layout="@layout/title_1"/>            <TextView        style="@style/match_parent"        android:gravity="center"        android:text="@string/phone_location_display_position_describe"        android:textSize="20sp" />        <FrameLayout        android:id="@+id/fl"        style="@style/wrap_content"        android:background="@drawable/call_locate_green" >        <LinearLayout            android:layout_width="wrap_content"            android:layout_height="40dp"            android:orientation="horizontal" >            <ImageView                android:layout_width="25dp"                android:layout_height="25dp"                android:layout_gravity="center"                android:layout_marginLeft="10dp"                android:src="@drawable/icon" />            <TextView                style="@style/wrap_content"                android:layout_marginLeft="10dp"                android:text="北京移动"                android:textColor="@color/white"                android:textSize="25sp" />        </LinearLayout>    </FrameLayout></RelativeLayout>